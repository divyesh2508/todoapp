{
  "name": "nodejstodo",
  "version": "1.0.0",
  "description": "node js to do app",
  "main": "app.js",
  "scripts": {
  "test": "jest --coverage",
  "coverage": "jest --coverage --coverageReporters=lcov",
  "build": "babel src -d lib"
},
  "jest": {
    "testMatch": ["**/?(*.)+(spec|test).[tj]s?(x)"],
    "testPathIgnorePatterns": ["/node_modules/"],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!**/node_modules/**"
    ],
    "coverageReporters": ["text", "lcov"]
  },

  "author": "Divyesh",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.5",
    "express": "^4.18.2",
    "mongoose": "^5.11.7",
    "rollbar": "^2.26.4"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "mocha": "^10.7.0",
    "sonarqube-scanner": "^4.0.1",
    "supertest": "^7.0.0",
     "jest": "^27.0.0"
  }
}
